        scanf("%d\t%d\t%d\t",&a,&b,&c);
        if((a>b)&&(b>c))
        {
                printf("a is greater");
        }
        if(b>c)
        {
                printf("b is greater");
        }
        else 
        {
                 printf("b is greater");
         }
