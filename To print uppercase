#include<stdio.h>
#include<conio.h>
void main(void) 
{ 
        char character,increase;
        clrscr(); 
        printf("enter the character"); 
        scanf("%c",&character);
        for(increase=65;increase<=90;)
        {
              printf("%c",increase); 
              increase=increase+1;
        }
        getch(); 
  }    
       
